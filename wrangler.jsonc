{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "tools",
  "compatibility_date": "2025-10-11",
  "routes": [
    {
      "pattern": "tools.n-s.work",
      "custom_domain": true,
    },
  ],
  "build": {
    "command": "npx @11ty/eleventy",
  },
  "assets": {
    "directory": "./dist",
  },
  "observability": {
    "enabled": true,
  },
}
